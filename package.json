{
  "name": "ng2-slimscroll",
  "version": "1.2.6",
  "description": "Angular2 Customizable Scrollbar",
  "main": "bundle/ng2-slimscroll.umd.js",
  "module": "ng2-slimscroll.js",
  "typings": "ng2-slimscroll.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkuri/ng2-slimscroll.git"
  },
  "scripts": {
    "clean": "./scripts/clean.sh",
    "dev": "tsc --watch",
    "build": "./node_modules/.bin/ngc -p tsconfig.json && rollup -c",
    "lint": "tslint ./src/*.ts",
    "prepublish": "npm run lint && npm run build"
  },
  "keywords": [
    "ng2",
    "angular",
    "angular2",
    "scroll",
    "scrollbar",
    "slimscroll"
  ],
  "author": "Jan Kuri <jkuri88@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^2.4.9",
    "@angular/compiler": "^2.4.9",
    "@angular/compiler-cli": "^2.4.9",
    "@angular/core": "^2.4.9",
    "@angular/platform-browser": "^2.4.9",
    "@types/node": "7.0.5",
    "codelyzer": "^3.0.0-beta.0",
    "core-js": "^2.4.1",
    "reflect-metadata": "0.1.10",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rxjs": "5.2.0",
    "tslint": "^4.5.1",
    "typescript": "2.2.1",
    "zone.js": "^0.7.7"
  }
