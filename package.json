{
  "name": "ng2-slimscroll",
  "version": "1.0.2",
  "description": "Angular2 Customizable Scrollbar Implementation",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkuri/ng2-slimscroll.git"
  },
  "scripts": {
    "clean": "./scripts/clean.sh",
    "dev": "tsc --watch",
